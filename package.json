{
  "name": "webpack-demo",
  "sideEffects": [
    "*.css"
  ],
  "version": "1.0.0",
  "description": "webpack-demo",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack-dev-server --inline --progress --colors --watch --compress --config ./config/webpack.dev.js",
    "theme": "webpack --watch --config ./config/webpack.theme.js",
    "dll:dev": "webpack --progress --colors --config ./config/webpack.dev.dll.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liviuscn/webpack-demo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/liviuscn/webpack-demo/issues"
  },
  "homepage": "https://github.com/liviuscn/webpack-demo#readme",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.1",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  }
}
